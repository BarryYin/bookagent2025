# AI 模型配置
# Gemini API Key (如果使用 Google Gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI/OpenRouter API Key (如果使用 OpenAI 或 OpenRouter)
OPENAI_API_KEY=sk-your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4

# 百度 ERNIE API Key (如果使用百度千帆)
BAIDU_API_KEY=your_baidu_api_key_here
BAIDU_BASE_URL=https://qianfan.baidubce.com/v2
BAIDU_MODEL=ernie-4.5-turbo-32k

# Qwen API Key (如果使用阿里通义千问)
QWEN_API_KEY=your_qwen_api_key_here
QWEN_BASE_URL=https://api-inference.modelscope.cn/v1/
QWEN_MODEL=Qwen/Qwen3-Coder-480B-A35B-Instruct

# 讯飞语音合成 API
XUNFEI_APP_ID=your_xunfei_app_id
XUNFEI_API_KEY=your_xunfei_api_key
XUNFEI_API_SECRET=your_xunfei_api_secret

# 星火大模型 API
SPARKAI_APP_ID=your_sparkai_app_id
SPARKAI_API_KEY=your_sparkai_api_key
SPARKAI_API_SECRET=your_sparkai_api_secret

# Fish Audio API (语音生成)
FISH_API_KEY=your_fish_api_key
FISH_REFERENCE_ID=your_fish_reference_id

# JWT 认证密钥
JWT_SECRET_KEY=your_random_secret_key_here
JWT_ALGORITHM=HS256

# 百度 OCR API
BAIDU_OCR_API_KEY=your_baidu_ocr_api_key
BAIDU_OCR_SECRET_KEY=your_baidu_ocr_secret_key

# 应用配置
DEBUG=False
HOST=0.0.0.0
PORT=8000
